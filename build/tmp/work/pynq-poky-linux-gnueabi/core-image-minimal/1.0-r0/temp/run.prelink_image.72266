	linuxloader_glibc
}

bbnote() {
	if [ -p /home/student/Desktop/yocto_pynq/Yocto_project_pynq_board/build/tmp/work/pynq-poky-linux-gnueabi/core-image-minimal/1.0-r0/temp/fifo.72266 ] ; then
		printf "%b\0" "bbnote $*" > /home/student/Desktop/yocto_pynq/Yocto_project_pynq_board/build/tmp/work/pynq-poky-linux-gnueabi/core-image-minimal/1.0-r0/temp/fifo.72266
	else
		echo "NOTE: $*"
	fi
}

linuxloader_glibc() {
	case arm in
		powerpc | microblaze )
			dynamic_loader="/lib/ld.so.1"
			;;
		mipsisa32r6el | mipsisa32r6 | mipsisa64r6el | mipsisa64r6)
			dynamic_loader="/lib/ld-linux-mipsn8.so.1"
			;;
		mips* )
			dynamic_loader="/lib/ld.so.1"
			;;
		powerpc64)
			dynamic_loader="/lib/ld64.so.1"
			;;
		x86_64)
			dynamic_loader="/lib/ld-linux-x86-64.so.2"
			;;
		i*86 )
			dynamic_loader="/lib/ld-linux.so.2"
			;;
		arm )
			dynamic_loader="/lib/ld-linux.so.3"
			;;
		* )
			dynamic_loader="/unknown_dynamic_linker"
			;;
	esac
	echo $dynamic_loader
}

