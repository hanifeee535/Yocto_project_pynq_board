	echo "CONFIG_LOCALVERSION="\"-xilinx-v2017.3\" >> /home/student/Desktop/yocto_pynq/Yocto_project_pynq_board/build/tmp/work/pynq-poky-linux-gnueabi/linux-xlnx/4.9-xilinx-v2017.3+gitAUTOINC+f1b1e077d6-r0/linux-pynq-standard-build/.config
